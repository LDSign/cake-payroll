{
    "name": "ldsign/cake-payroll",
    "description": "Payroll plugin for CakePHP",
    "homepage": "https://github.com/ldsign/cake-payroll",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
        "name": "LDSign",
        "email": "info@ldsign.de"
    }],
	"support": {
		"issues": "https://github.com/ldsign/cake-payroll/issues",
		"source": "https://github.com/ldsign/cake-payroll"
	},
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "autoload": {
        "psr-4": {
            "Payroll\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Payroll\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}